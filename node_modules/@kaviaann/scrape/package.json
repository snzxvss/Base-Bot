{
  "name": "@kaviaann/scrape",
  "version": "1.0.3",
  "description": "A Scrape with full optimization from the before version ( @kaviaann/scraper )",
  "type": "module",
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "debug:reset": "rm -rf ./lib && npm run debug",
    "ts": "tsc",
    "debug": "tsc && node index.js",
    "build": "npx rollup -c",
    "publish": "npx rollup -c && npm publish --access public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.4.1",
    "@types/ws": "^8.5.12",
    "@types/yt-search": "^2.10.3",
    "rollup": "^4.21.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "cheerio": "^1.0.0-rc.12",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.0",
    "yt-search": "^2.12.1"
  }
}
